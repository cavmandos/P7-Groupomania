{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.regexp.test.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nimport Swal from 'sweetalert2';\nexport default {\n  name: \"SigninView\",\n  data: function data() {\n    return {\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      password: \"\",\n      nameRules: [function (v) {\n        return !!v || \"Ce champ est requis\";\n      }, function (v) {\n        return v && v.length <= 20 || \"Doit être inférieur à 20 caractères\";\n      }],\n      emailRules: [function (v) {\n        return !!v || \"Adresse email requise\";\n      }, function (v) {\n        return /^(([^<>()[\\]\\\\.,;:\\s@']+(\\.[^<>()\\\\[\\]\\\\.,;:\\s@']+)*)|('.+'))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(v) || \"Une adresse email valide est requise\";\n      }],\n      passwordRules: [function (v) {\n        return !!v || \"Un mot de passe est requis\";\n      }, function (v) {\n        return /(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,}/.test(v) || \"Le mot de passe doit contenir au moins 6 caractères, dont une majuscule, une minuscule, un chiffre et un caractère spécial\";\n      }]\n    };\n  },\n  methods: {\n    signin: function signin() {\n      var _this = this;\n\n      var prenom = this.firstName;\n      var nom = this.lastName;\n      var email = this.email;\n      var password = this.password;\n\n      if (prenom.length == 0 || nom.length == 0 || email.length <= 6 || password.length <= 5) {\n        Swal.fire({\n          title: 'Presque...',\n          text: 'Votre formulaire est incomplet',\n          icon: 'warning',\n          confirmButtonText: \"J'ai compris\",\n          iconColor: '#ffd7d7',\n          confirmButtonColor: '#955DAD'\n        });\n      } else {\n        var contact = {\n          prenom: prenom,\n          nom: nom,\n          email: email,\n          password: password\n        };\n        console.table(contact); // Send form with axios\n\n        axios({\n          method: \"post\",\n          url: \"http://localhost:3000/auth/register\",\n          data: contact,\n          headers: {\n            \"Content-Type\": \"application/json\"\n          }\n        }) // It works, redirection\n        .then(function (reponse) {\n          console.log(reponse);\n          Swal.fire({\n            title: 'Bienvenue !',\n            text: 'Votre inscription est validée',\n            icon: 'success',\n            confirmButtonText: \"Yes\"\n          });\n          localStorage.clear();\n\n          _this.$router.push(\"/\");\n        }).catch(function (err) {\n          alert(err.response.data.message);\n        });\n      }\n    },\n    goToLogin: function goToLogin() {\n      this.$router.push(\"/\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgFA;AACA;AACA;AACAA,oBADA;AAEAC,MAFA,kBAEA;AACA;AACAC,mBADA;AAEAC,kBAFA;AAGAC,eAHA;AAIAC,kBAJA;AAMAC,kBACA;AAAA;AAAA,OADA,EAEA;AAAA;AAAA,OAFA,CANA;AAUAC,mBACA;AAAA;AAAA,OADA,EAEA;AAAA,eACA,+JACAC,CADA,KAEA,sCAHA;AAAA,OAFA,CAVA;AAiBAC,sBACA;AAAA;AAAA,OADA,EAEA;AAAA,eACA,iDACA,4HAFA;AAAA,OAFA;AAjBA;AAwBA,GA3BA;AA6BAC;AACAC,UADA,oBACA;AAAA;;AACA;AACA;AACA;AACA;;AAEA,UACAC,sBACAC,eADA,IAEAT,iBAFA,IAGAC,oBAJA,EAKA;AACAS;AACAC,6BADA;AAEAC,gDAFA;AAGAC,yBAHA;AAIAC,2CAJA;AAKAC,8BALA;AAMAC;AANA;AAQA,OAdA,MAcA;AACA;AACAR,wBADA;AAEAC,kBAFA;AAGAT,sBAHA;AAIAC;AAJA;AAOAgB,+BARA,CAUA;;AACAC;AACAC,wBADA;AAEAC,oDAFA;AAGAvB,uBAHA;AAIAwB;AAAA;AAAA;AAJA,WAMA;AANA,SAOAC,IAPA,CAOA;AACAL;AACAP;AACAC,gCADA;AAEAC,iDAFA;AAGAC,2BAHA;AAIAC;AAJA;AAMAS;;AACA;AACA,SAjBA,EAkBAC,KAlBA,CAkBA;AACAC;AACA,SApBA;AAqBA;AACA,KAtDA;AAuDAC,aAvDA,uBAuDA;AACA;AACA;AAzDA;AA7BA","names":["name","data","firstName","lastName","email","password","nameRules","emailRules","v","passwordRules","methods","signin","prenom","nom","Swal","title","text","icon","confirmButtonText","iconColor","confirmButtonColor","console","axios","method","url","headers","then","localStorage","catch","alert","goToLogin"],"sourceRoot":"src/views","sources":["RegisterView.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div @click=\"goToLogin\" class=\"logoTitre\">\n      <img src=\"../assets/icon.png\" alt=\"Logo de Groupomania\" />\n      <h1>Groupomania</h1>\n    </div>\n    <v-main>\n      <v-container fluid fill-height>\n        <v-layout justify-center>\n          <v-flex xs12 sm8 md4>\n            <v-card class=\"elevation-12\">\n              <v-toolbar class=\"red lighten-4\">\n                <v-toolbar-title>Créer un compte</v-toolbar-title>\n              </v-toolbar>\n              <v-card-text>\n                <v-form\n                  ref=\"form\"\n                  action=\"/auth/register\"\n                  method=\"POST\"\n                  @submit.prevent=\"signin\"\n                >\n                  <v-text-field\n                    v-model=\"firstName\"\n                    name=\"firstName\"\n                    label=\"Prenom\"\n                    type=\"text\"\n                    placeholder=\"Votre prénom\"\n                    required\n                    :rules=\"nameRules\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"lastName\"\n                    name=\"lastName\"\n                    label=\"Nom\"\n                    type=\"text\"\n                    placeholder=\"Votre nom\"\n                    required\n                    :rules=\"nameRules\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"email\"\n                    name=\"email\"\n                    label=\"Email\"\n                    type=\"text\"\n                    placeholder=\"Votre adresse email\"\n                    required\n                    :rules=\"emailRules\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"password\"\n                    name=\"password\"\n                    label=\"Mot de passe\"\n                    type=\"password\"\n                    placeholder=\"Votre mot de passe\"\n                    required\n                    :rules=\"passwordRules\"\n                  ></v-text-field>\n                  <div class=\"btnCtn\">\n                    <v-btn\n                      type=\"submit\"\n                      class=\"mt-4 rounded-pill\"\n                      color=\"red lighten-4\"\n                      value=\"register\"\n                      >S'inscrire</v-btn\n                    >\n                  </div>\n                </v-form>\n              </v-card-text>\n            </v-card>\n          </v-flex>\n        </v-layout>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from 'sweetalert2'\nexport default {\n  name: \"SigninView\",\n  data() {\n    return {\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      password: \"\",\n\n      nameRules: [\n        (v) => !!v || \"Ce champ est requis\",\n        (v) => (v && v.length <= 20) || \"Doit être inférieur à 20 caractères\",\n      ],\n      emailRules: [\n        (v) => !!v || \"Adresse email requise\",\n        (v) =>\n          /^(([^<>()[\\]\\\\.,;:\\s@']+(\\.[^<>()\\\\[\\]\\\\.,;:\\s@']+)*)|('.+'))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(\n            v\n          ) || \"Une adresse email valide est requise\",\n      ],\n      passwordRules: [\n        (v) => !!v || \"Un mot de passe est requis\",\n        (v) =>\n          /(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,}/.test(v) ||\n          \"Le mot de passe doit contenir au moins 6 caractères, dont une majuscule, une minuscule, un chiffre et un caractère spécial\",\n      ],\n    };\n  },\n\n  methods: {\n    signin() {\n      let prenom = this.firstName;\n      let nom = this.lastName;\n      let email = this.email;\n      let password = this.password;\n\n      if (\n        prenom.length == 0 ||\n        nom.length == 0 ||\n        email.length <= 6 ||\n        password.length <= 5\n      ) {\n        Swal.fire({\n          title: 'Presque...',\n          text: 'Votre formulaire est incomplet',\n          icon: 'warning',\n          confirmButtonText: \"J'ai compris\",\n          iconColor: '#ffd7d7',\n          confirmButtonColor:'#955DAD',\n        })\n      } else {\n        let contact = {\n          prenom: prenom,\n          nom: nom,\n          email: email,\n          password: password,\n        };\n\n        console.table(contact);\n\n        // Send form with axios\n        axios({\n          method: \"post\",\n          url: \"http://localhost:3000/auth/register\",\n          data: contact,\n          headers: { \"Content-Type\": \"application/json\" },\n        })\n          // It works, redirection\n          .then((reponse) => {\n            console.log(reponse);\n            Swal.fire({\n              title: 'Bienvenue !',\n              text: 'Votre inscription est validée',\n              icon: 'success',\n              confirmButtonText: \"Yes\"\n            });\n            localStorage.clear();\n            this.$router.push(\"/\");\n          })\n          .catch((err) => {\n            alert(err.response.data.message);\n          });\n      }\n    },\n    goToLogin() {\n      this.$router.push(\"/\");\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.logoTitre {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\nimg {\n  width: 80px;\n  &:hover {\n    cursor: pointer;\n  }\n}\n\nh1:hover {\n  cursor: pointer;\n}\n\n.btnCtn {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}