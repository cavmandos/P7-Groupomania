      <v-form v-model=\"valid\" ref=\"form\" action=\"/auth/register\" method=\"POST\" @submit.prevent=\"signin\">\n                  <v-text-field\n                    v-model=\"firstName\"\n                    name=\"firstName\"\n                    label=\"Prenom\"\n                    type=\"text\"\n                    placeholder=\"Votre prénom\"\n                    required\n                    :rules=\"nameRules\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"lastName\"\n                    name=\"lastName\"\n                    label=\"Nom\"\n                    type=\"text\"\n                    placeholder=\"Votre nom\"\n                    required\n                    :rules=\"nameRules\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"email\"\n                    name=\"email\"\n                    label=\"Email\"\n                    type=\"text\"\n                    placeholder=\"Votre adresse email\"\n                    required\n                    :rules=\"emailRules\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"password\"\n                    name=\"password\"\n                    label=\"Mot de passe\"\n                    type=\"password\"\n                    placeholder=\"Votre mot de passe\"\n                    required\n                    :rules=\"passwordRules\"\n                  ></v-text-field>\n                  <div class=\"btnCtn\">\n                    <v-btn\n                      type=\"submit\"\n                      class=\"mt-4 rounded-pill\"\n                      color=\"red lighten-4\"\n                      value=\"register\"\n                      >S'inscrire</v-btn\n                    >\n                  </div>\n                </v-form>\n              </v-card-text>\n            </v-card>\n          </v-flex>\n        </v-layout>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"SigninView\",\n  data() {\n    return {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        password: \"\",\n\n        nameRules: [\n          v => !!v || 'Ce champ est requis',\n          v => (v && v.length <= 20) || 'Doit être inférieur à 20 caractères',\n        ],\n        emailRules: [\n          v => !!v || 'E-mail is required',\n          v => /^(([^<>()[\\]\\\\.,;:\\s@']+(\\.[^<>()\\\\[\\]\\\\.,;:\\s@']+)*)|('.+'))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(v) || \"Une adresse email valide est requise\",\n        ],\n        passwordRules: [\n          v => !!v || 'Password is required',\n          v => /(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,}/.test(v) || 'Password must contain at least lowercase letter, one number, a special character and one uppercase letter',\n        ],\n    };\n  },\n\n  methods: {\n    signin() {\n      let prenom = this.firstName;\n      let nom = this.lastName;\n      let email = this.email;\n      let password = this.password;\n\n      let contact = {\n        prenom: prenom,\n        nom: nom,\n        email: email,\n        password: password,\n      }\n\n      //console.table(contact);\n\n      // Envoi du formulaire via la méthode post d'axios\n      axios({\n        method: \"post\",\n        url: \"http://localhost:3000/auth/register\",\n        data: contact,\n        headers: { \"Content-Type\": \"application/json\" },\n      })\n        // Inscription réussite, maj du localStorage et redirection sur le Login\n        .then((reponse) => {\n          console.log(reponse);\n          alert(\n            \"Inscription réussite\"\n          );\n          //localStorage.clear();\n        })\n        .catch((err) => {\n          alert(err.response.data.message);\n        });\n    },\n    goToLogin() {\n      this.$router.push(\"/\");\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.logoTitre {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\nimg {\n  width: 80px;\n  &:hover {\n    cursor: pointer;\n  }\n}\n\nh1:hover {\n  cursor: pointer;\n}\n\n.btnCtn {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}